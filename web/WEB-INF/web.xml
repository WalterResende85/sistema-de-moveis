<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PesquisaClienteController</servlet-name>
        <servlet-class>controller.PesquisaClienteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaFuncionarioController</servlet-name>
        <servlet-class>controller.PesquisaFuncionarioController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaFornecedorController</servlet-name>
        <servlet-class>controller.PesquisaFornecedorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaMovelController</servlet-name>
        <servlet-class>controller.PesquisaMovelController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaPedidoController</servlet-name>
        <servlet-class>controller.PesquisaPedidoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaMaterialController</servlet-name>
        <servlet-class>controller.PesquisaMaterialController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PesquisaFerramentaController</servlet-name>
        <servlet-class>controller.PesquisaFerramentaController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PesquisaClienteController</servlet-name>
        <url-pattern>/PesquisaClienteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaFuncionarioController</servlet-name>
        <url-pattern>/PesquisaFuncionarioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaFornecedorController</servlet-name>
        <url-pattern>/PesquisaFornecedorController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaMovelController</servlet-name>
        <url-pattern>/PesquisaMovelController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaPedidoController</servlet-name>
        <url-pattern>/PesquisaPedidoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaMaterialController</servlet-name>
        <url-pattern>/PesquisaMaterialController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PesquisaFerramentaController</servlet-name>
        <url-pattern>/PesquisaFerramentaController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
